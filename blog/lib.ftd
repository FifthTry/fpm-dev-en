-- end: ftd.column

-- end: ftd.column

-- end: ftd.column

-- end: ftd.column

-- end: faq











-- component question-accordion: 
caption title: Hi
body body:
boolean $open: false
ftd.image-src src1: $fastn-assets.files.images.blog.faq.down.svg
ftd.image-src src2: $fastn-assets.files.images.blog.faq.up.svg

-- ftd.column:
width: fill-container
margin-bottom.px: 23
background.solid: $inherited.colors.background.step-1
align-content: center
classes: markdown

-- ftd.row:
;; background.solid: $fonts.eeeeee80
width: fill-container
spacing: space-between
padding-horizontal.px: 15
padding-horizontal.px if { ftd.device == "mobile" } : 12
padding-vertical.px: 15
$on-click$: $ftd.toggle($a = $question-accordion.open)

-- ftd.text: $question-accordion.title
;; role: $fonts.nunito-18-18-700
color if { question-accordion.open }: $inherited.colors.custom.four
color: $inherited.colors.custom.one
align-self: center

-- ftd.image:
if: { !question-accordion.open }
src: $question-accordion.src1
width.fixed.px: 12
height.fixed.px: 14

-- ftd.image:
if: { question-accordion.open }
src: $question-accordion.src2
width.fixed.px: 12
height.fixed.px: 14

-- end: ftd.row

-- ftd.column:
if: { question-accordion.open }
padding-vertical.px: 15
padding-horizontal.px: 15
width: fill-container

-- ftd.text:
;; role: $fonts.size-18
role if { ftd.device == "mobile" }: $inherited.types.fine-print
color: $inherited.colors.custom.one

$question-accordion.body

-- end: ftd.column

-- end: ftd.column

-- end: question-accordion


