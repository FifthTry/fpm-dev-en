optional caption title:
children child:

-- ftd.column:
width: fill-container

-- ftd.column:
width: fill-container
align-content: center
background.solid: $inherited.colors.background.step-1
padding-top.px if { ftd.device == "desktop" }: 76
padding-bottom.px if { ftd.device == "desktop" }: 37


-- ftd.column:
width: fill-container
