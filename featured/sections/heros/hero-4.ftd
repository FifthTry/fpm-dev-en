discord-url: https://discord.gg/bucrdvptYd
copyright: Copyright Â© 2023 - [FifthTry.com](https://www.fifthtry.com/)

-- end: ds.page.footer

-- ds.page.layout-bar:

-- ds.layout: Hero 4
cta-text: User Manual
cta-url: https://fastnui.trizwit.com/UI-Components/Hero/hero-4/
previous-url: /featured/sections/heros/
github-url: https://github.com/Trizwit/FastnUI/

-- end: ds.page.layout-bar


-- ds.preview-card:
image: $fastn-assets.files.images.featured.components.hero-4.png
cta-url: https://fastnui.trizwit.com/UI-Components/Hero/hero-4/
cta-text: Demo

-- ft-ui.grid-view: Featured Heros
if: { show-grid }
