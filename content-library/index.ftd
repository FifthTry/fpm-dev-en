-- ftd.text: $footer-list-child.title
if: { footer-list-child.url != NULL }
role: $inherited.types.copy-regular
color if { footer-list-child.mouse-in }: $inherited.colors.accent.primary
$on-mouse-leave$: $ftd.set-bool($a = $footer-list-child.mouse-in, v = false)
$on-mouse-enter$: $ftd.set-bool($a = $footer-list-child.mouse-in, v = true)
color: $inherited.colors.text
link: $footer-list-child.url
width: fill-container
	
/-- footer-list-child: $obj.title
if: { !ftd.is_empty(footer-list-child.children) }
url: $obj.url
is-active: $obj.is-active
children: $obj.children
$loop$: $footer-list-child.children as $obj

-- end: ftd.column

-- end: footer-list-child








